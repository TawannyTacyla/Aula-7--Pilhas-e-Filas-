# Implementa칞칚o da Classe Pilha 
class Pilha:
    def __init__(self):
        self.itens = []

    def push(self, item):
        self.itens.append(item)

    def pop(self):
        if not self.esta_vazia():
            return self.itens.pop()

    def esta_vazia(self):
        return len(self.itens) == 0

# Classe Navegador que simula o hist칩rico de navega칞칚o
class Navegador:
    def __init__(self, pagina_inicial):
        self.historico_voltar = Pilha()
        self.historico_avancar = Pilha()
        self.pagina_atual = pagina_inicial
        print(f"游빐 Navegador iniciado em: {self.pagina_atual}")

    def visitar_pagina(self, url):
        self.historico_voltar.push(self.pagina_atual)  # Armazena a p치gina atual antes de visitar outra
        self.pagina_atual = url  # Atualiza para a nova p치gina
        self.historico_avancar = Pilha()  # Limpa o hist칩rico de avan칞ar
        print(f"游깷 Visitando: {self.pagina_atual}")

    def voltar(self):
        if not self.historico_voltar.esta_vazia():
            self.historico_avancar.push(self.pagina_atual)  # Guarda a p치gina atual
            self.pagina_atual = self.historico_voltar.pop()  # Volta  p치gina anterior
            print(f"拘勇 Voltando para: {self.pagina_atual}")
        else:
            print("丘멆잺 N칚o h치 p치ginas para voltar.")

    def avancar(self):
        if not self.historico_avancar.esta_vazia():
            self.historico_voltar.push(self.pagina_atual)  # Guarda a p치gina atual
            self.pagina_atual = self.historico_avancar.pop()  # Avan칞a para a pr칩xima p치gina
            print(f"俱뫮잺 Avan칞ando para: {self.pagina_atual}")
        else:
            print("丘멆잺 N칚o h치 p치ginas para avan칞ar.")

# Demonstra칞칚o do simulador de navega칞칚o
navegador = Navegador("google.com")
navegador.visitar_pagina("youtube.com")
navegador.visitar_pagina("Sigaa.ufpe.br")  
navegador.voltar()
navegador.voltar()
navegador.avancar()
navegador.visitar_pagina("github.com")
navegador.voltar()
navegador.avancar()
